package java8.stream.api.model;

public class Product {
	public int id;
	// visibility of data type to public it's not feasible solution but for practicing
    public String name;  
    public float price;  
    public Product(int id, String name, float price) {  
        this.id = id;  
        this.name = name;  
        this.price = price;  
    }
	public int getId() {
		return id;
	}
	public String getName() {
		return name;
	}
	public float getPrice() {
		return price;
	}
    
    
}
